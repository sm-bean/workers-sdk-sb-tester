{
  "name": "format-errors",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "wrangler build",
    "check:lint": "eslint . --max-warnings=0",
    "check:type": "tsc",
    "start": "wrangler dev",
    "deploy": "CLOUDFLARE_ACCOUNT_ID=$WORKERS_NEW_CLOUDFLARE_ACCOUNT_ID CLOUDFLARE_API_TOKEN=$WORKERS_NEW_CLOUDFLARE_API_TOKEN wrangler deploy",
    "dev": "wrangler dev --port=8766",
    "test": "vitest -c tests/vitest.config.mts",
    "test:ci": "vitest run -c tests/vitest.config.mts",
    "e2e:container": "pnpm wrangler pages dev tests/containerapp --port=8765",
    "e2e:fixture": "npx concurrently -s first -k  \"pnpm run dev\" \"pnpm run e2e:container\""
  },
  "devDependencies": {
    "@cloudflare/eslint-config-worker": "workspace:*",
    "@cloudflare/vitest-pool-workers": "workspace:*",
    "@cloudflare/workers-types": "^4.20240605.0",
    "@playwright/test": "^1.45.0",
    "@types/node": "20.8.3",
    "miniflare": "workspace:*",
    "mustache": "^4.2.0",
    "playwright": "^1.45.0",
    "promjs": "^0.4.2",
    "toucan-js": "^3.3.1",
    "typescript": "^5.4.5",
    "vitest": "1.5.0",
    "wrangler": "workspace:*",
    "zod": "^3.22.3",
    "concurrently": "^8.2.2"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
