		"test:ci": "vitest run -c tests/vitest.config.mts"
	},
	"devDependencies": {
		"@cloudflare/eslint-config-worker": "workspace:*",
		"@cloudflare/vitest-pool-workers": "workspace:*",
		"@cloudflare/workers-types": "^4.20240605.0",
		"mustache": "^4.2.0",
		"promjs": "^0.4.2",
		"toucan-js": "^3.3.1",
		"zod": "^3.22.3",
		"@types/node": "20.8.3",
		"miniflare": "workspace:*",
		"typescript": "^5.4.5",
		"vitest": "1.5.0",
		"wrangler": "workspace:*"
	},
	"volta": {
		"extends": "../../package.json"
	}
}